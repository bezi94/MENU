        <div>
       
            <form id="odb_1faz" action="">
               
                <div>
                    Podaj moc znamionow¹ (W): <input type="number" name="moc_znamionowa"><br />
                    Podaj napiêcie znamionowe (V): <input type="number" name="napiecie" value="230"><br />
                    <button onclick=" return odb_1faz()" style="background-color:orange">Oblicz</button>


                </div>
            </form>
            <script>
                // Przekroj dla odbiornika 1 fazowego
                function odb_1faz() {
                    //przypisanie obiektu moc_znamionowa do zmiennej
                    var pole_moc = document.forms['odb_1faz'].moc_znamionowa;
                    //odczytanie mocy
                    var moc_znamionowa = pole_moc.value;

                    //przypisanie obiektu napiecie do zmiennej
                    var pole_napiecie = document.forms['odb_1faz'].napiecie;
                    //odczytanie napiecia
                    var napiecie = pole_napiecie.value;
                    // obliczanie pradu obciazenia
                    var prad;
                    prad = moc_znamionowa / napiecie;
                    //alert(prad);
                    var prad_pop = prad.toFixed(2);
                    if (moc_znamionowa > 3680) {
                        alert("Pr¹d powy¿ej 16A, potrzebne zasilanie 3-fazowe");
                    } else if (prad_pop > 9.5) {
                        alert(prad_pop + "A. Zastosuj przewod 2,5mm i zab. 16A.");
                    } else if (prad_pop < 9.5) {
                        alert(prad_pop + "A. Zastosuj przewod 1,5mm i zab 10A.");
                    }
                    return false;
                }
            </script>
             Pr¹d znamionowy odbiornika rezystancyjnego 3 fazowego
            <form id="odb_3faz" action="">
                <div>
                    Podaj moc znamionow¹ (W): <input type="number" name="moc_znamionowa"><br />
                    Podaj napiêcie miêdzyprzewodowe (V): <input type="number" name="napiecie3f" value="400"><br />
                    <button onclick=" return odb_3faz()" style="background-color:orange">Oblicz</button>
                </div>
            </form>
            <script>
                // Przekroj dla odbiornika 1 fazowego
                function odb_3faz() {
                    //przypisanie obiektu moc_znamionowa do zmiennej
                    var pole_moc = document.forms['odb_3faz'].moc_znamionowa;
                    //odczytanie mocy
                    var moc_znamionowa = pole_moc.value;

                    //przypisanie obiektu napiecie do zmiennej
                    var pole_napiecie = document.forms['odb_3faz'].napiecie3f;
                    //odczytanie napiecia
                    var napiecie3f = pole_napiecie.value;
                    // obliczanie pradu obciazenia
                    var prad;
                    prad = moc_znamionowa / (Math.sqrt(3) * napiecie3f);
                    var prad_pop = prad.toFixed(2);
                    if (prad_pop < 13) {
                        alert(prad_pop + "A. Przewod 1,5mm i zab. 13A.");
                    } else if (prad_pop < 16) {
                        alert(prad_pop + "A. Przewod 2,5mm i zab. 16A.");
                    } else if (prad_pop < 25) {
                        alert(prad_pop + "A. Przewod 4mm i zab. 25A.");
                    } else if (prad_pop < 32) {
                        alert(prad_pop + "A. Przewod 6mm i zab. 32A.");
                    } else if (prad_pop < 40) {
                        alert(prad_pop + "A. Przewod 10mm i zab 40A.");
                    } else if (prad_pop < 50) {
                        alert(prad_pop + "A. Przewod 16mm i zab. 50A.");
                    } else if (prad_pop < 63) {
                        alert(prad_pop + "A. Przewod 25mm i zab. 63A.");
                    } else if (prad_pop < 80) {
                        alert(prad_pop + "A. Przewod 35mm i zab. 80A.");
                    } else {
                        alert("Wynik: " + prad_pop + "A. Mam wiedze tylko do 80 A.");
                    }
                    return false;
                }
            </script>
            // pr¹d znamionowy silnika 1 fazowego
            <form id="sil_1faz" action="">
                <div>
                    Podaj moc znamionow¹ (W): <input type="number" name="moc_znamionowa"><br />
                    Podaj napiêcie znamionowe (V): <input type="number" name="napiecie" value="230"><br />
                    Podaj sprawnoœæ znamionow¹ silnika: <input type="number" name="sprawnosc" placeholder="0 - 1"><br />
                    Podaj wspó³czynnik mocy znamionowej: <input type="number" name="wspolczynnik" placeholder="0.75 - 0.9"><br />
                    <button onclick=" return sil_1faz()" style="background-color:orange">Oblicz</button>
                </div>
            </form>
            <script>
                // Przekroj dla odbiornika 1 fazowego
                function sil_1faz() {
                    //przypisanie obiektu moc_znamionowa do zmiennej
                    var pole_moc = document.forms['sil_1faz'].moc_znamionowa;
                    //odczytanie mocy
                    var moc_znamionowa = pole_moc.value;

                    //przypisanie obiektu napiecie do zmiennej
                    var pole_napiecie = document.forms['sil_1faz'].napiecie;
                    //odczytanie napiecia
                    var napiecie = pole_napiecie.value;

                    // przypisanie obiektu sprawnosc do zmiennej
                    var pole_sprawnosc = document.forms['sil_1faz'].sprawnosc;
                    //odczytanie sprawnosci
                    var sprawnosc = pole_sprawnosc.value;

                    // przypisanie obiektu wspolczynnik do zmiennej
                    var pole_wspolczynnik = document.forms['sil_1faz'].wspolczynnik;
                    // odczyt sprawnosci
                    var wspolczynnik = pole_wspolczynnik.value;


                    // obliczanie pradu obciazenia
                    var prad;
                    prad = moc_znamionowa / (napiecie * sprawnosc * wspolczynnik);
                    //alert(prad +" " + mianownik);
                    var prad_pop = prad.toFixed(2);
                    if (moc_znamionowa > 3680) {
                        alert("Pr¹d powy¿ej 16A, potrzebne zasilanie 3-fazowe");
                    } else if (prad_pop > 9.5) {
                        alert(prad_pop + "A. Zastosuj przewod 2,5mm i zab. 16A.");
                    } else if (prad_pop < 9.5) {
                        alert(prad_pop + "A. Zastosuj przewod 1,5mm i zab 10A.");
                    }

                    return false;
                }
            </script>
            // pr¹d znamionowy silnika 3 fazowego
            <form id="sil_3faz" action="">
                <div>
                    Podaj moc znamionow¹ (W): <input type="number" name="moc_znamionowa"><br />
                    Podaj napiêcie miêdzyprzewodowe (V): <input type="number" name="napiecie3f" value="400"><br />
                    Podaj sprawnoœæ znamionow¹ silnika: <input type="number" name="sprawnosc" placeholder="0 - 1"><br />
                    Podaj wspó³czynnik mocy znamionowej: <input type="number" name="wspolczynnik" placeholder="0.75 - 0.9"><br />
                    <button onclick=" return sil_3faz()" style="background-color:orange">Oblicz</button>
                </div>
            </form>
            <script>
                // Przekroj dla odbiornika 1 fazowego
                function sil_3faz() {
                    //przypisanie obiektu moc_znamionowa do zmiennej
                    var pole_moc = document.forms['sil_3faz'].moc_znamionowa;
                    //odczytanie mocy
                    var moc_znamionowa = pole_moc.value;

                    //przypisanie obiektu napiecie do zmiennej
                    var pole_napiecie = document.forms['sil_3faz'].napiecie3f;
                    //odczytanie napiecia
                    var napiecie3f = pole_napiecie.value;

                    // przypisanie obiektu sprawnosc do zmiennej
                    var pole_sprawnosc = document.forms['sil_3faz'].sprawnosc;
                    //odczytanie sprawnosci
                    var sprawnosc = pole_sprawnosc.value;

                    // przypisanie obiektu wspolczynnik do zmiennej
                    var pole_wspolczynnik = document.forms['sil_3faz'].wspolczynnik;
                    // odczyt sprawnosci
                    var wspolczynnik = pole_wspolczynnik.value;

                    // obliczanie pradu obciazenia

                    var prad;
                    prad = moc_znamionowa / ((Math.sqrt(3) * napiecie3f) * sprawnosc * wspolczynnik);

                    var prad_pop = prad.toFixed(2); // ograniczenie do 2 miejsc po przecinku
                    if (prad_pop < 13) {
                        alert(prad_pop + "A. Przewod 1,5mm i zab. 13A.");
                    } else if (prad_pop < 16) {
                        alert(prad_pop + "A. Przewod 2,5mm i zab. 16A.");
                    } else if (prad_pop < 25) {
                        alert(prad_pop + "A. Przewod 4mm i zab. 25A.");
                    } else if (prad_pop < 32) {
                        alert(prad_pop + "A. Przewod 6mm i zab. 32A.");
                    } else if (prad_pop < 40) {
                        alert(prad_pop + "A. Przewod 10mm i zab 40A.");
                    } else if (prad_pop < 50) {
                        alert(prad_pop + "A. Przewod 16mm i zab. 50A.");
                    } else if (prad_pop < 63) {
                        alert(prad_pop + "A. Przewod 25mm i zab. 63A.");
                    } else if (prad_pop < 80) {
                        alert(prad_pop + "A. Przewod 35mm i zab. 80A.");
                    } else {
                        alert("Wynik: " + prad_pop + "A. Mam wiedze tylko do 80 A.");
                    }
                    return false;
                }
            </script>
